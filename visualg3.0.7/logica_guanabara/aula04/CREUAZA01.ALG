Algoritmo "creuaza01"

Var
    ano_atual, ano_nasc, idade: inteiro

Inicio
    escreva(" Em que ano nós estamos? ")
    leia(ano_atual)
    escreva(" Em que ano nós estamos? ")
    leia(ano_nasc)
    
    idade <- ano_atual - ano_nasc
    
    escreval("Minha idade será: ", idade)


Fimalgoritmo