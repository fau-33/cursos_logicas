Algoritmo "contar_usuario"

Var
   valor, salto, contador: inteiro

Inicio
   contador <- 0
   escreva("Quer contar até quanto? ")
   leia(valor)
   escreva("Qual o valor do salto? ")
   leia(salto)
   enquanto (contador <= valor) faca
        escreval(contador)
      contador <- contador + salto
   fimEnquanto
      escreval("Terminiei de contar")


Fimalgoritmo