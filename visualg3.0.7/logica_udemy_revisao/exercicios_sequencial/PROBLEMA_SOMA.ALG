Algoritmo "PROBLEMA_SOMA"

Var
   X, Y, SOMA: inteiro


Inicio
      // Entrada de dados
     escreva("Digite o valor de X: ")
     leia(X)
     escreva("Digite o valor de Y: ")
     leia(Y)
     
     // Processamento de dados
     SOMA <- X + Y
     
     // Saída de dados
     escreval("SOMA = ", SOMA)
     
     

Fimalgoritmo