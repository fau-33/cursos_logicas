Algoritmo "problema_consumo"

Var

   distanciaPercorrida, combustivel, consumo: real



Inicio
   escreva("Distancia percorrida: ")
   leia(distanciaPercorrida)
   escreva("Combustível gasto: ")
   leia(combustivel)

   consumo <- distanciaPercorrida / combustivel

   escreva("Consumo médio = ", consumo:5:3)


Fimalgoritmo